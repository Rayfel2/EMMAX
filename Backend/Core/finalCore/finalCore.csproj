    <UserSecretsId>5a61f211-fbbf-4f0f-b0ba-c1fd6b438de5</UserSecretsId>
    <DockerDefaultTargetOS>Windows</DockerDefaultTargetOS>
  </PropertyGroup>

  <ItemGroup>
